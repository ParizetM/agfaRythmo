APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# === Fonctionnalités IA ===
# Activer/désactiver les fonctionnalités IA (true/false)
AI_SCENE_DETECTION_ENABLED=true
AI_DIALOGUE_EXTRACTION_ENABLED=false
AI_TRANSLATION_ENABLED=false

# === Extraction dialogues (Whisper + Diarization) ===
# Modèle Whisper : tiny (1GB RAM), base (1GB), small (2GB)
AI_WHISPER_MODEL=tiny

# Diarization (séparation locuteurs)
AI_DIARIZATION_ENABLED=false
AI_DIARIZATION_METHOD=resemblyzer
AI_MAX_SPEAKERS=10

# Séparation vocale avec Demucs (améliore qualité diarization)
# ⚠️ Nécessite 4GB+ RAM - Désactiver si serveur < 4GB
AI_VOCAL_SEPARATION_ENABLED=false

# === Traduction automatique (NLLB-200) ===
# Provider : nllb (Meta AI, 200 langues, local, gratuit) ⭐⭐⭐⭐
AI_TRANSLATION_PROVIDER=nllb
AI_NLLB_MODEL_SIZE=600M
AI_TRANSLATION_USE_CONTEXT=true

# Token HuggingFace (pour télécharger modèles NLLB)
# Gratuit : https://huggingface.co/settings/tokens
HF_TOKEN=


