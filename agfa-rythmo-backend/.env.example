APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Fonctionnalités IA
# Activer/désactiver les fonctionnalités IA (true/false)
AI_SCENE_DETECTION_ENABLED=true

# Configuration extraction de dialogues
# Activer l'extraction automatique avec Whisper (nécessite Python + whisper)
AI_DIALOGUE_EXTRACTION_ENABLED=false

# Modèle Whisper à utiliser : tiny (rapide, 1GB RAM), base (1GB), small (2GB), medium (5GB), large (10GB)
# Recommandé pour serveur 2GB RAM : tiny ou base
AI_WHISPER_MODEL=tiny

# Activer la diarization (séparation locuteurs) - nécessite pyannote.audio
# Si désactivé, tous les dialogues sont assignés à un seul speaker
AI_DIARIZATION_ENABLED=false

# Nombre maximum de locuteurs à détecter (2-20)
AI_MAX_SPEAKERS=10

# Token HuggingFace pour diarization (GRATUIT)
# 1. Créer compte sur https://huggingface.co
# 2. Générer token: https://huggingface.co/settings/tokens
# 3. Accepter conditions: https://huggingface.co/pyannote/speaker-diarization-3.1
HF_TOKEN=

# Configuration traduction automatique
# Activer la traduction des dialogues (nécessite connexion internet uniquement)
AI_TRANSLATION_ENABLED=false

# Provider de traduction (choisir UN seul) :
# - deepl : ⭐⭐⭐⭐⭐ MEILLEURE QUALITÉ, 500k chars/mois gratuit, API key requise
# - google : ⭐⭐⭐⭐ Très bonne qualité, Google Cloud API, API key requise
# - mymemory : ⭐⭐⭐ Bonne qualité, GRATUIT 10k/jour, AUCUNE config (RECOMMANDÉ DÉBUTANTS)
# - libretranslate : ⭐⭐⭐ Open-source, API publique gratuite ou auto-hébergée
# Fournisseur de traduction :
# - nllb : Meta AI - 200 langues, gratuit, local (~2GB) ⭐⭐⭐⭐ (RECOMMANDÉ)
# - mymemory : Gratuit 10k/jour, simple, limite 500 chars ⭐⭐⭐
AI_TRANSLATION_PROVIDER=nllb

# === Configuration NLLB-200 (si AI_TRANSLATION_PROVIDER=nllb) ===
# Meta AI - 200 langues, gratuit, local (~2GB)
# Taille du modèle : 600M (recommandé), 1.3B, 3.3B
AI_NLLB_MODEL_SIZE=600M

# Utiliser contexte personnages pour améliorer qualité traduction
AI_TRANSLATION_USE_CONTEXT=true


